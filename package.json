{
  "name": "poczprojectapp",
  "version": "0.0.1",
  "private": true,
  "main": "./build/index.js",
  "cracoConfig": "craco.config.js",
  "scripts": {
    "start": "PORT=4201 craco start",
    "build": "craco build",
    "test": "craco test --colors --coverage --watchAll=false --no-cache",
    "eject": "react-scripts eject",
    "lint": "eslint '*/**/*.{js,ts,tsx}' --fix --quiet",
    "prepare": "husky install"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/components/**/*.{js,ts,tsx}",
      "src/libs/**/*.{js,ts,tsx}",
      "src/pages/**/*.{js,ts,tsx}",
      "src/services/**/*.{js,ts,tsx}",
      "!src/pages/auth/pages/subscription/**/*.{js,ts,tsx}",
      "!src/pages/auth/pages/plans/features/**/*.{js,ts,tsx}",
      "!src/pages/layout/context/**/*.{js,ts,tsx}",
      "!src/**/index.ts",
      "!<rootDir>/node_modules/"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": "yarn lint"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "resolutions": {
    "@types/react": "17.0.2",
    "@types/react-dom": "17.0.2"
  },
  "dependencies": {
    "@craco/craco": "^6.4.0",
    "@hookform/resolvers": "^2.8.10",
    "@testing-library/dom": "^8.10.1",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/faker": "^5.5.9",
    "@types/history": "^4.7.9",
    "@types/jest": "^28.1.2",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router": "^5.1.18",
    "axios": "^0.24.0",
    "axios-mock-adapter": "^1.20.0",
    "crypto-js": "^4.1.1",
    "luxon": "^2.0.2",
    "rc-slider": "^10.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-dropzone": "^14.2.1",
    "react-easy-crop": "^4.2.0",
    "react-hook-form": "^7.18.0",
    "react-number-format": "^4.9.3",
    "react-router-dom": "^5.3.0",
    "react-scripts": "^5.0.1",
    "react-textarea-autosize": "^8.3.4",
    "slugify": "^1.6.5",
    "typescript": "^4.1.2",
    "web-vitals": "^1.0.1",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.2",
    "@types/axios": "^0.14.0",
    "@types/crypto-js": "^4.1.1",
    "@types/luxon": "^2.0.5",
    "@types/mercadopago": "^1.5.5",
    "@types/react-router-dom": "^5.3.2",
    "@types/tailwindcss": "^3.0.10",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "autoprefixer": "^10.4.7",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jest": "^26.5.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "faker": "^5.5.3",
    "husky": "^7.0.4",
    "lint-staged": "^11.2.5",
    "postcss": "^8.4.14",
    "tailwindcss": "^3.0.24",
    "ts-jest": "^28.0.4"
  }
}
